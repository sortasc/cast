<div id="packery" ng-controller='BoardsController'>
  <div class="item mood"><img src='/assets/icons/dead.png'></div>

  <div class="item events">
  	<h6>Events</h6>
  	<div class="content"></div>
  </div>
  
  <div class="item notes">
  	<h6>Notes</h6>
  	<div class='content'></div>
  </div>

  <div class="item score">
    <input class='dial' data-min='-100' data-bgColor='#27ae60' data-fgColor='#2ecc71' data-thickness='.4' data-displayPrevious=true value='{{ score }}'>
  </div>

  <div class="item timeline nano">
		<%= render 'timeline' %>
  </div>
  
  <div class="item feed" id='feed'>
  	<h6>News</h6>
  	<div class="content"></div>
  </div>

  <div class="item chat">
  	<h6>Chat</h6>
  	<div class="content"></div>
  </div>
</div>
